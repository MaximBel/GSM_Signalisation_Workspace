#ifndef GSM_M950E_H_
#define GSM_M950E_H_


#include "stdint.h"
#include "stddef.h"

typedef enum {
	OutCalling_Refused,
	OutCalling_Connected,
	OutCalling_Error
} OutCalling_t;

typedef struct {
	void (*IncommingCall)(char *Number);
} ModemHandlers_t;

uint8_t gsm_init(ModemHandlers_t handlers);

void CallToNumber(char *Number, uint16_t Timeout);

OutCalling_t GetCallingState(void);



#endif /* GSM_M950E_H_ */
